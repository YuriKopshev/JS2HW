{  "plugins": [
    "prettier",
    "unicorn"
  ],
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [ 
      "plugin:prettier/recommended",
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier"
  ],
    "overrides": [
        {
            "files": ".prettierrc",
            "parser": "json" 
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error", 
        { "endOfLine":"auto" }], 
        "react/prop-types": 0, 
        "semi": [2, "always"]
    }    
}
