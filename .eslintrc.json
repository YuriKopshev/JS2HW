{  "plugins": [
    "prettier",
    "unicorn"
  ],
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [ 
      "plugin:prettier/recommended",
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier"
  ],
    "overrides": [
        {
            "files": ".prettierrc",
            "parser": "json" 
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    }
}
